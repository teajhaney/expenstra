{
  "expo": {
    "name": "Expenstra",
    "slug": "expenstra",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "expenstra",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
	
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.expenstra",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#4F46E5"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.expenstra"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-community/datetimepicker",
	  [
        "expo-splash-screen",
        {
          "backgroundColor": "#4f46e5",
          "image": "./assets/images/splash-icon.png",
          "dark": {
            "image": "./assets/images/splash-icon.png",
            "backgroundColor": "#4f46e5"
          },
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "eac182b4-9eef-4c67-9628-aaec15e4254c"
      }
    }
  }
}
